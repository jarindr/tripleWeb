{
  "hosting": {
    "public": "build",
    "headers": [
      {
        "source" : "./**/*",
        "headers" : [ {
          "key" : "Cache-Control",
          "value" : "max-age=31536000"
          } ]
        }
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  }
